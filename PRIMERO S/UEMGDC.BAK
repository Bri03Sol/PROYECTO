CLEAR
CADENA= "UNIDAD EDUCATIVA MANUELA GARAICOA DE CALDERÓN"
 *PRIMERA PARTE
 ?SUBSTR(CADENA,1, AT(" ", CADENA,1)-1)
 
 *SEGUNDA PARTE
 ?SUBSTR(CADENA,AT(" ", CADENA,1)+1, AT(" ",CADENA,2)- AT(" ",CADENA,1))
 
 *TERCERA PARTE
 ?SUBSTR(CADENA,AT(" ",CADENA,2)+1,AT(" ",CADENA,3)- AT(" ", CADENA,2))
 
 *CUARTA PARTE
 ?SUBSTR(CADENA,AT(" ",CADENA,3)+1,AT(" ",CADENA,4)- AT(" ", CADENA,3))
 
 *QUINTA PARTE
 
 ?SUBSTR(CADENA,AT(" ",CADENA,4)+1,AT(" ",CADENA,5))