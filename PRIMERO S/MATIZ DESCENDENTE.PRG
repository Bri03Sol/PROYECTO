CLEAR 

DECLARE APELLIDOS (5)
FOR N=1 TO 5
APELLIDOS(N)=SPACE(40)
@10,20 GET APELLIDOS(N)
READ 
ENDFOR

*MOSTRAR LA MATRIZ ANTES DE ORDENAR
?"MATRIZ ORIGINAL"
FOR N=1 TO 5
?APELLIDOS(N)
ENDFOR

ASORT(APELLIDOS)

*INVERTIR MATRIZ
LCAPELLIDOS=" "
FOR N=1 TO INT(5/2)
LCAPELLDOS=APELLIDOS(N)
APELLIDOS(N)=APELLIDOS(6-N)
APELLIDOS(6-N)=LCAPELLIDOS
ENDFOR

*MOSTRAR LA MATRIZ ORDENADA

?"MATRIZ ORDENADA"
FOR N=1 TO 5
?APELLIDOS(N)
ENDFOR